<%# Moved out of _search_header. %>
<%= render 'constraints' %>

<div id="sidebar">
  <div class="container">
    <div class="row">
      <nav class="col-md-12" role="navigation">
        <%= render 'search_sidebar' %>
      </nav>
    </div>
  </div>
</div>

<main id="content" class="container-fluid">
  <%# Added container-fluid for a bit more padding so margins align. %>
  <% unless has_search_parameters? %>
    <%# We should never get here, because we redirect to add the access facet. -%>
    <%= render 'home' %>
  <% else %>
    <%= render 'search_results' %>
  <% end %>
</main>
